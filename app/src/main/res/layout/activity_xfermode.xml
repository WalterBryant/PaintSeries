<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:stretchColumns="2">

        <TableRow>

            <Button
                android:id="@+id/rounddstinbtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="圆角" />

            <Button
                android:id="@+id/invertdstinbtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="倒影图像" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/irregularwavebtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="不规则wave" />

            <Button
                android:id="@+id/heartbitbtn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="心电图" />
        </TableRow>

        <TableRow>

            <Button
                android:id="@+id/lightbookview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="书架" />

            <Button
                android:id="@+id/twitterview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="小鸟" />
        </TableRow>
    </TableLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="2">

        <TableRow>
            <Button
                android:id="@+id/roundsrcin"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="圆角"/>

            <Button
                android:id="@+id/invertsrcin"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="倒影图像"/>
        </TableRow>

        <TableRow>
            <Button
                android:id="@+id/eraserview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="橡皮擦"/>
            <Button
                android:id="@+id/guaguaview"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="刮刮卡"/>
        </TableRow>

        <TableRow>
            <Button
                android:id="@+id/roundsrcatop"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="圆角"/>
            <Button
                android:id="@+id/invertsrcatop"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="倒影图像"/>
        </TableRow>
    </TableLayout>

    <com.paint.series.xfermode.dst.RoundImageViewDSTIN
        android:id="@+id/roundimage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.paint.series.xfermode.dst.InvertedImageViewDSTIN
        android:id="@+id/invertimg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.paint.series.xfermode.dst.IrregularWaveViewDSTIN
        android:id="@+id/irregularwaveview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.paint.series.xfermode.dst.HeartMapDSTIN
        android:id="@+id/heartbitview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.paint.series.xfermode.other.LightBookViewLIGHTEN
        android:id="@+id/lightbookview_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.paint.series.xfermode.other.TwitterViewMULTIPLY
        android:id="@+id/twitterview_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />





    <!--xfermode src start-->
    <com.paint.series.xfermode.src.RoundImageViewSRCIN
        android:id="@+id/roundsrcin_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <com.paint.series.xfermode.src.InvertedImageViewSRCIN
        android:id="@+id/invertsrcin_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <com.paint.series.xfermode.src.EraserViewSRCOUT
        android:id="@+id/eraserview_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <com.paint.series.xfermode.src.GuaGuaCardViewSRCOUT
        android:id="@+id/guaguaview_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <com.paint.series.xfermode.src.RoundImageViewSRCATOP
        android:id="@+id/roundsrcatop_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <com.paint.series.xfermode.src.InvertedImageViewSRCATOP
        android:id="@+id/invertsrcatop_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
    <!--xfermode src end-->
</LinearLayout>
